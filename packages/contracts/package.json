{
  "name": "contracts",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "----- EXTERNAL -----": "---------------------------",
    "initialize": "pnpm tablegen && pnpm build && pnpm worldgen && pnpm worldtypes && pnpm tsgen",
    "devnode": "mud devnode",
    "build": "forge clean && forge build",
    "test": "mud test-v2",
    "dev": "pnpm run deploy && node ./openDevUrl.js",
    "deploy": "pnpm run initialize && mud deploy",
    "deploy:testnet": "pnpm run initialize && mud deploy --profile=lattice-testnet",
    "lint": "pnpm prettier && pnpm solhint",
    "tablegen": "mud tablegen",
    "worldgen": "mud worldgen",
    "worldtypes": "rimraf types && (typechain --target=ethers-v5 out/IWorld.sol/IWorld.json || typechain --target=ethers-v5 out/world/IWorld.sol/IWorld.json)",
    "tsgen": "mud tsgen --configPath mud.config.mts --out ../client/src/mud",
    "----- INTERNAL -----": "---------------------------",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@latticexyz/cli": "2.0.0-alpha.1.195",
    "@latticexyz/config": "2.0.0-alpha.1.195",
    "@latticexyz/schema-type": "2.0.0-alpha.1.195",
    "@latticexyz/solecs": "2.0.0-alpha.1.195",
    "@latticexyz/std-contracts": "2.0.0-alpha.1.195",
    "@latticexyz/store": "2.0.0-alpha.1.195",
    "@latticexyz/world": "2.0.0-alpha.1.195",
    "@solidstate/contracts": "^0.0.52",
    "@typechain/ethers-v5": "^10.1.1",
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#c9ce3f25bde29fc5eb9901842bf02850dfd2d084",
    "ethers": "^5.7.2",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#b4f121555729b3afb3c5ffccb62ff4b6e2818fd3",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "run-pty": "^4.0.3",
    "solhint": "^3.3.7",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {}
}
